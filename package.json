{
  "name": "@zkpayroll/sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for ZK Payroll on Stellar",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "stellar",
    "soroban",
    "zk",
    "zero-knowledge",
    "payroll",
    "privacy",
    "blockchain"
  ],
  "author": "ZK Payroll Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/zk-payroll-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/zk-payroll-sdk/issues"
  },
  "homepage": "https://github.com/your-org/zk-payroll-sdk#readme",
  "dependencies": {
    "@stellar/stellar-sdk": "^12.0.0",
    "circomlibjs": "^0.1.7",
    "snarkjs": "^0.7.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.1.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
